{
    "version": 2,
    "builds":[
        {
            "src":"index.py",
            "use": "@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.py"
        }
    ],
    "functions":
    {
        "*":
        {
            "memory": 3008,
            "maxDuration": 60
        }
    }
}
